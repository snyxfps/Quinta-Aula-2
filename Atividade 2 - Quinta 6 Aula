public class Main {
    public static void main(String[] args) {
        int numero = 6;
        int resultado = verificarPar(numero);
        System.out.println(resultado);
    }

    public static int verificarPar(int numero) {
        if (numero % 2 == 0) {
            return 1; // número par
        } else {
            return 2; // número ímpar
        }
    }
}
